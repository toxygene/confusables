{
    "name": "toxygene/confusables",
    "description": "This library is an implementation of the skeleton function described in the Confusion Detection section of the Unicode Security Mechanisms technical standard.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Justin Hendrickson",
            "email": "justin.hendrickson@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "nikic/php-parser": "^3.1",
        "symfony/polyfill-mbstring": "^1.7"
    },
    "autoload": {
        "psr-4": {
            "Confusables\\": "src"
        }
    },
    "bin": [
        "bin/build-confusables"
    ],
    "scripts": {
        "post-package-install": [
            "@build"
        ],
        "post-package-update": [
            "@build"
        ],
        "build": [
            "bin/build-confusables"
        ],
        "test": [
            "phpunit"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "friendsofphp/php-cs-fixer": "^2.10"
    }
}
